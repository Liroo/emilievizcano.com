{
  "name": "emilievizcano.com",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next",
    "format": "npx prettier --write . --ignore-path .gitignore --cache",
    "lint": "next lint -- --ignore-path .gitignore",
    "lint:fix": "npm run format && npm run lint -- --fix",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "prettier": {
    "importOrder": [
      "^components/(.*)$",
      "^icons/(.*)$",
      "^images/(.*)$",
      "^flux/(.*)$",
      "^types/(.*)$",
      "^hooks/(.*)$",
      "^services/(.*)$",
      "^utils/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "jsxSingleQuote": false,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "printWidth": 80,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false
  },
  "dependencies": {
    "@portabletext/react": "3.0.11",
    "@reduxjs/toolkit": "^2.1.0",
    "@sanity/client": "6.12.3",
    "@sanity/image-url": "1.0.2",
    "@sanity/orderable-document-list": "^1.2.1",
    "@sanity/overlays": "2.3.9",
    "@sanity/preview-url-secret": "1.5.5",
    "@sanity/vision": "3.27.1",
    "@vercel/og": "0.6.2",
    "animejs": "^3.2.2",
    "classnames": "2.5.1",
    "date-fns": "2.30.0",
    "framer-motion": "^11.0.3",
    "intl-segmenter-polyfill": "0.4.4",
    "next": "14.1.0",
    "next-redux-wrapper": "^8.1.0",
    "next-sanity": "7.0.17",
    "next-sanity-image": "6.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "reselect": "^5.1.0",
    "sanity": "3.27.1",
    "sanity-plugin-asset-source-unsplash": "1.1.2",
    "sanity-plugin-iframe-pane": "3.1.3"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/react": "18.2.51",
    "@types/styled-components": "5.1.34",
    "autoprefixer": "10.4.17",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "prettier-plugin-packagejson": "2.4.10",
    "prettier-plugin-tailwindcss": "0.5.11",
    "redux-logger": "^3.0.6",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=16"
  }
}
